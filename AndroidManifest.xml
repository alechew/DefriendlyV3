<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.fsu.cs.muppetlabs.defriendly"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/defriendly_launch_logo"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.NoTitleBar" >
        <activity
            android:screenOrientation="portrait"
			android:configChanges="orientation"
            android:name="edu.fsu.cs.muppetlabs.defriendly.HomeActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <meta-data 
            android:name="com.facebook.sdk.ApplicationId" 
            android:value="@string/app_id"/>
        <activity 
            android:screenOrientation="portrait"
			android:configChanges="orientation"
            android:name="com.facebook.LoginActivity">
             </activity>
        <activity android:name="GetUsers"
            android:screenOrientation="portrait"
			android:configChanges="orientation">
            
            
        </activity>
        <activity android:name="EndGame"
            android:screenOrientation="portrait"
			android:configChanges="orientation">
          </activity>
        
    </application>

</manifest>
